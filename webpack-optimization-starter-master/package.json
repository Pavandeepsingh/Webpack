{
  "name": "webpack-optimization-starter-master",
  "version": "1.0.0",
  "description": "Learning Webpack",
  "main": "index.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=dev npx webpack serve --config webpack/webpack.dev.config.js --hot",
    "build": "cross-env NODE_ENV=production npx webpack --config webpack/webpack.prod.config.js",
    "typecheck": "tsc --noEmit",
    "bundle-analyzer": "cross-env NODE_ENV=production npx webpack --config webpack/webpack.analyzer.config.js",
    "start": "cross-env NODE_ENV=dev node src/server.js",
    "start-prod": "npm run build && node src/server.js"
  },
